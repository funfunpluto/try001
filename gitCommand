# https://www.git-tower.com/learn/git/commands
#https://www.youtube.com/watch?v=SWYqp7iY_Tc

git init
git config --global user.name "funfunpluto"
git config --global user.email funfunpluto@gmail.com

git add filename
git status
git commit (-a all, -m "message")
git rm --cache filename

#add to .gitignore any file or directory you don't want git to include

#create anthor branch
git branch branchname

git checkout branchname
#create a new filename

git checkout master
#the new file added or edited under new branch won't show

git merge login

git remote
git remote add origin https://github.com/funfunpluto/try001.git
git remote

git push -u origin master

touch README.md
git add .
git commit -m 'Added readme'
git push



#Fetch the remote master branch and check it out when Updates were rejected because the remote contains work that you do not have locally.

git fetch origin master
git checkout master

#when Updates were rejected because the tip of your current branch is behind
git pull origin master
git push origin master


